    "mini-css-extract-plugin": "^2.7.3",
    "mocha": "^10.7.3",
    "nyc": "^17.1.0",
    "process": "^0.11.10",
    "remove-files-webpack-plugin": "^1.5.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "typings-for-css-modules-loader": "^1.7.0",
    "util": "^0.12.5",
