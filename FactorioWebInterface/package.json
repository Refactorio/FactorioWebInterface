    "bulma": "0.8.0",
    "deep-equal": "^2.2.3"
  },
  "devDependencies": {
    "@types/assert": "^1.5.10",
    "@types/deep-equal": "^1.0.4",
    "@types/mocha": "^10.0.8",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "html-webpack-plugin": "^5.6.0",
